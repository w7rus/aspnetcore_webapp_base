{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Hangfire": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "GoogleReCaptchaV2Options": {
    "PrivateKey": ""
  },
  "JsonWebTokenOptions": {
    "Issuer": "",
    "ValidateIssuer": false,
    "Audience": "",
    "ValidateAudience": false,
    "IssuerSigningKey": "",
    "ValidateIssuerSigningKey": true,
    "ValidateLifetime": false,
    "ExpirySeconds": 0
  },
  "RefreshTokenOptions": {
    "DefaultExpirySeconds": 0
  },
  "BackgroundServicesOptions": {
    "JsonWebTokenBackgroundServiceOptions": {
      "CheckUpdateTimeSeconds": 300
    }
  },
  "MiscOptions": {
    "SecureCookies": true,
    "FileServer": {
      "Scheme": "",
      "Host": "",
      "Port": 0,
      "Path": ""
    },
    "CDNServers": [
      {
        "Scheme": "",
        "Host": "",
        "Port": 0,
        "Path": ""
      }
    ]
  },
  "ConnectionStrings": {
    "Default": "",
    "Test": "",
    "Hangfire": "",
    "HangfireTest": ""
  }
}
